{
  "rules": {
    "no-duplicate-variable": true,
    "prefer-const": true,
    "no-null-keyword": true,
    "no-require-imports": true,
    "arrow-return-shorthand": true,
    "import-spacing": true,
    "no-eval": true,
    "prefer-template": [
      true,
      "allow-single-concat"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "indent": [
      true,
      "spaces",
      4
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
