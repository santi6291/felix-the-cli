{
  "services": {
    "scss": {
      "main": [{
        "path": "/src/scss",
        "template": "./templates/scss/app.hb"
      }, {
        "path": "/src/scss/core",
        "template": "./templates/scss/mixins.hb"
      }, {
        "path": "/src/scss/core",
        "template": "./templates/scss/functions.hb"
      }, {
        "path": "/src/scss/core",
        "template": "./templates/scss/settings.hb"
      }, {
        "path": "/src/scss/core",
        "template": "./templates/scss/settings.hb"
      }],
      "categories": {
        "block": {
          "path": "/blocks",
          "template": "./templates/scss/blocks.hb"
        },
        "components": {
          "path": "/components",
          "template": "./templates/scss/components.hb"
        },
        "utility": {
          "path": "/utilities",
          "template": "./templates/scss/utilities.hb"
        }
      }
    },
    "ts": {
      "main": [{
        "path": "/src/ts",
        "template": "./templates/ts/app.hb"
      }],
      "categories": {
        "block": {
          "path": "/blocks",
          "template": "./templates/ts/blocks.hb"
        },
        "components": {
          "path": "/components",
          "template": "./templates/ts/components.hb"
        },
        "service": {
          "path": "/service",
          "template": "./templates/ts/service.hb"
        }
      }
    },
    "twig": {
      "main": [{
        "path": "/src/views",
        "template": "./templates/twig/main.hb"
      }],
      "categories": {
        "block": {
          "path": "/blocks",
          "template": "./templates/twig/blocks.hb"
        },
        "components": {
          "path": "/components",
          "template": "./templates/twig/components.hb"
        }
      }
    }
  }
}
