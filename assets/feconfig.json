{
  "services": {
    "scss": {
      "extension": "scss",
      "main": [
        {
          "path": "./src/scss",
          "template": "./assets/templates/scss/app.hb"
        },
        {
          "path": "./src/scss/core",
          "template": "./assets/templates/scss/mixins.hb"
        },
        {
          "path": "./src/scss/core",
          "template": "./assets/templates/scss/functions.hb"
        },
        {
          "path": "./src/scss/core",
          "template": "./assets/templates/scss/settings.hb"
        },
        {
          "path": "./src/scss/core",
          "template": "./assets/templates/scss/settings.hb"
        }
      ],
      "categories": {
        "block": {
          "path": "/blocks",
          "template": "./assets/templates/scss/blocks.hb"
        },
        "components": {
          "path": "/components",
          "template": "./assets/templates/scss/components.hb"
        },
        "utility": {
          "path": "/utilities",
          "template": "./assets/templates/scss/utilities.hb"
        }
      }
    },
    "ts": {
      "extension": "ts",
      "main": [
        {
          "path": "./src/ts",
          "template": "./assets/templates/ts/app.hb"
        }
      ],
      "categories": {
        "block": {
          "path": "/blocks",
          "template": "./assets/templates/ts/blocks.hb"
        },
        "components": {
          "path": "/components",
          "template": "./assets/templates/ts/components.hb"
        },
        "service": {
          "path": "/service",
          "template": "./assets/templates/ts/service.hb"
        }
      }
    },
    "twig": {
      "extension": "twig",
      "main": [
        {
          "path": "./src/views",
          "template": "./assets/templates/twig/main.hb"
        }
      ],
      "categories": {
        "block": {
          "path": "/blocks",
          "template": "./assets/templates/twig/blocks.hb"
        },
        "components": {
          "path": "/components",
          "template": "./assets/templates/twig/components.hb"
        }
      }
    }
  }
}
